#todos-lists {
  height: $heightM;
  overflow-y: auto;
  >ul {
    padding: 0;
    margin: 0;
    list-style: none;
    font-size: .9rem;
    >li {
      padding: .5rem;
      &:hover{
        cursor: pointer;
        background-color: $supColorLightBlue;
      }
      >div {
        float: right;
        .active {
          color: $warningColor;
        }
      }
    }
  }
}